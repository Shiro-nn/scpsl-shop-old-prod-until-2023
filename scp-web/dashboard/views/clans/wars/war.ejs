<style>
body{
margin: 0;
color: #fff;
font-family: -apple-system,BlinkMacSystemFont,Roboto,Oxygen-Sans,Ubuntu,Cantarell,Helvetica Neue,sans-serif;
text-rendering: optimizeLegibility;
}
h1, h2, h3{margin: 0;}

.no-spinner{
padding-top: 100px;
position: fixed;
left: 50%;
transform: translateX(-50%);
}

.spinner{
--spinner-scale: 1;
width: 40px;
height: 40px;
margin: auto;
position: relative;
backface-visibility: hidden;
animation: spinner-3 1s cubic-bezier(.4,0,0,1) infinite;
}
.spinner::before, .spinner::after {
content: '';
background: #fff;
height: 8px;
width: 8px;
position: absolute;
border-radius: 50px;
top: 16px;
backface-visibility: hidden;
}
.spinner::before {
left: 0;
animation: spinner-1 1s cubic-bezier(.2,0,0,.2) infinite
}
.spinner::after {
left: 15px;
animation: spinner-2 1s cubic-bezier(.4,0,0,1) infinite;
box-shadow: 15px 0 0 0 #fff;
}
@keyframes spinner-1 {
0% {transform: scale(1) rotate(0.01deg)}
30% {transform: scale(1.4) rotate(0.01deg)}
100% {transform: scale(1) rotate(0.01deg)}
}
@keyframes spinner-2 {
0% {opacity: 1;transform: translate(0,0) rotate(0.01deg)}
40% {opacity: .5}
100% {opacity: 1;transform: translate(-45px,0) rotate(0.01deg)}
}
@keyframes spinner-3 {
0% {transform: translate(0,0) rotate(0.01deg)}
100% {transform: translate(30px,0) rotate(0.01deg)}
}

.content{
padding: 0 15vw;
padding-top: 110px;
}
@media only screen and (max-width: 1640px) {
    .content{
        padding-left: min(246px, 5vw);
        padding-right: min(246px, 5vw);
    }
}
@media only screen and (max-width: 1272px) {
    .images{
        grid-column: span 12/span 12 !important;
        border-top: solid 1px #fff !important;
        border-bottom:0!important;
        border-left:0!important;
        border-right:0!important;
        border-image: linear-gradient(to right, #ed28ff, #2897ff) 30 30 stretch !important;
    }
    .inf{
        grid-column: span 12/span 12 !important;
    }
    .img-cont img{
        height: min(170px, 100%) !important;
    }
}
.info{
display: grid;
grid-template-columns: repeat(12,minmax(0,1fr));
background-color: #333d4d;
border-radius: 16px;
}
.images{
display: flex;
justify-content: center;
grid-column: span 9/span 9;
border-left: solid 1px #fff;
border-image: linear-gradient(to bottom, #ed28ff, #2897ff)  1 100%;
background: radial-gradient(#4a5972, #333d4d);
padding: calc(var(--client-x, 500px) / 50) 0;
padding-left: 1px;
border-top-right-radius: 16px;
border-bottom-right-radius: 16px;
}
.img-cont{
width: 90%;
position: relative;
-webkit-user-select: none;
user-select: none;
}
.img-cont .counter{
border-radius: 16px;
background-color: #3e4147;
padding: 5px;
margin: 5px;
position: absolute;
right: 0;
}
.img-cont img{
position: relative;
width: 100%;
height: 505px;
border-radius: 16px;
text-indent: -9999px;
background-size: cover;
object-fit: cover;
}
.img-cont .slider{
position: absolute;
top: 0;
height: calc(100% - 15px);
width: 30px;
margin: 5px;
cursor: url(/cursors/red/pointer.cur), pointer;
background-size: 50%;
background-repeat: no-repeat;
background-position: center;
}
.img-cont .left{
left: 0;
background-image: url(/img/clans/slider-left.svg);
}
.img-cont .right{
right: 0;
background-image: url(/img/clans/slider-right.svg);
}
.img-cont .spinner{
position: absolute;
top: 50%;
left: 50%;
margin: 0;
transform:translateX(-50%) translateX(-50%);
}
.inf{
display: flex;
justify-content: center;
grid-column: span 3/span 3;
}
.inf-container{
width: 90%;
padding: 20px 10px;
}
.inf-container .owner-info .clan-tag{
font-weight: 300;
font-style: normal;
font-family: montserrat,sans-serif;
list-style: none;
color: #fff;
touch-action: manipulation;
box-sizing: border-box;
user-select: auto;
font-size: 1rem;
line-height: 1.25rem;
background: #52555e;
border-radius: 2px;
border: 1px solid #52555e;
display: inline-block;
height: 28px;
position: relative;
text-decoration: none;
width: 64px;
}
.inf-container .owner-info .clan-tag .clan-name{
font-style: normal;
font-family: montserrat,sans-serif;
list-style: none;
line-height: 1.25rem;
touch-action: manipulation;
box-sizing: border-box;
color: white !important;
overflow: hidden;
display: block;
font-size: .875rem;
font-weight: 700;
letter-spacing: 1px;
margin-top: 2px;
text-align: center;
text-indent: 2px;
text-transform: uppercase;
width: 100%;
